   "source": [
    "def reverse_array(arr):\n",
    "    n = len(arr)\n",
    "    \n",
    "    for i in range(n//2):\n",
    "        arr[i], arr[n-i-1] = arr[n-i-1], arr[i]\n",
    "\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "print(\"Original array:\", arr)\n",
    "reverse_array(arr)\n",
    "print(\"Reversed array:\", arr)"
   ]
  },
